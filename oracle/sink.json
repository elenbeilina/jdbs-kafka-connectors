{
  "name": "jdbc-sink-kafka-connector",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "topics": "main",
    "connection.url":"jdbc:postgresql://pg-db:5432/postgres",
    "connection.user": "adidas",
    "connection.password": "pas",
    "auto.create":true,

    "value.converter.schemas.enable": "false",

    "transforms": "xml_value, xml_flattener",
    "transforms.xml_value.type": "com.github.jcustenborder.kafka.connect.transform.xml.FromXml$Value",
    "transforms.xml_value.schema.path": "file:///usr/share/xsd-schemas/ASN.xsd",
    "transforms.xml_flattener.type": "com.aqualen.xmlflattener.XmlFlattener"
  }
}